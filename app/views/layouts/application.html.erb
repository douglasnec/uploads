<!DOCTYPE html>
<html>
<head>
  <title>UltimatumTeste</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if !flash[:notice].nil? %>
<div class="alert"><%= flash[:notice]%></div>
<% end %>
<% if !flash[:alert].nil? %>
<div class="alert"><%= flash[:alert]%></div>
<% end %>
<%= yield %>

</body>
</html>
